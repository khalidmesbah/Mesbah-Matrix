{
  "extends": [
    "prettier",
    "next",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "no-undef": "off",
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "react/no-children-prop": "off"
  },
  "plugins": [
    "@tanstack/query"
  ]
}
