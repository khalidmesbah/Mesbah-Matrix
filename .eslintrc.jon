{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "next/typescript",
    "plugin:react/recommended",
    "prettier",
    "plugin:@tanstack/eslint-plugin-query/recommended"
  ],
  "plugins": ["react", "@tanstack/query"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off",
    "no-undef": "off"
  }
}
