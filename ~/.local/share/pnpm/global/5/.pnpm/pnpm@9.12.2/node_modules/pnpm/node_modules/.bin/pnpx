#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules/pnpm/bin/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules/pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/node_modules:/home/khalidmesbah/repos/mesbah-matrix/node_modules:/home/khalidmesbah/repos/node_modules:/home/khalidmesbah/node_modules:/home/node_modules:/node_modules:~/.local/share/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules/pnpm/bin/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules/pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/pnpm@9.12.2/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/.pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/5/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/global/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/pnpm/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/share/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/.local/node_modules:/home/khalidmesbah/repos/mesbah-matrix/~/node_modules:/home/khalidmesbah/repos/mesbah-matrix/node_modules:/home/khalidmesbah/repos/node_modules:/home/khalidmesbah/node_modules:/home/node_modules:/node_modules:~/.local/share/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/pnpx.cjs" "$@"
else
  exec node  "$basedir/../../bin/pnpx.cjs" "$@"
fi
